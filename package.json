{
  "name": "github-owner-repos",
  "description": "Get the github repositories for the specified owner or array of owners.",
  "version": "0.1.0",
  "homepage": "https://github.com/doowb/github-owner-repos",
  "author": "Brian Woodward (https://github.com/doowb)",
  "repository": "doowb/github-owner-repos",
  "bugs": {
    "url": "https://github.com/doowb/github-owner-repos/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "LICENSE"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "co": "^4.6.0",
    "extend-shallow": "^2.0.1",
    "github-base": "^0.4.1"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.10"
  },
  "keywords": [
    "github",
    "org",
    "org-github-repositories",
    "org-repos",
    "org-repositories",
    "organization",
    "organization-repositories",
    "organizations",
    "organziation",
    "organziation-github-repositories",
    "orgs",
    "owner",
    "owner-github-repositories",
    "owner-repositories",
    "owners",
    "repos",
    "repositories",
    "stars",
    "user",
    "user-github-repositories",
    "user-repos",
    "user-repositories",
    "users"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    },
    "related": {
      "list": [
        "github-base"
      ]
    },
    "reflinks": [
      "verb",
      "verb-generate-readme"
    ]
  }
}
